{
  "name": "cmake-runtime",
  "version": "3.29.3-0",
  "description": "Prebuilt CMake binaries for macOS, Linux, and Windows",
  "main": "index.js",
  "bin": {
    "ccmake": "bin/ccmake",
    "cmake": "bin/cmake",
    "cmcldeps": "bin/cmcldeps",
    "cpack": "bin/cpack",
    "ctest": "bin/ctest"
  },
  "files": [
    "NOTICE",
    "index.js",
    "bin",
    "lib"
  ],
  "imports": {
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    }
  },
  "scripts": {
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/cmake-runtime.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/cmake-runtime/issues"
  },
  "homepage": "https://github.com/holepunchto/cmake-runtime#readme",
  "dependencies": {
    "bare-os": "^2.2.1",
    "bare-path": "^2.1.1"
  },
  "optionalDependencies": {
    "cmake-runtime-darwin-universal": "3.29.2",
    "cmake-runtime-linux-arm64": "3.29.2",
    "cmake-runtime-linux-x64": "3.29.2",
    "cmake-runtime-win32-arm64": "3.29.2",
    "cmake-runtime-win32-x64": "3.29.2"
  },
  "devDependencies": {
    "standard": "^17.0.0"
  }
}
